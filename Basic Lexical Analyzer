#include<iostream>
#include<cmath>
using namespace std;
bool isDigit(char ch) 
{
		if (ch >= 48 && ch <= 57) 
         return true; 
      return false;
}
int main()
{

	 char s;
	cout<<"Enter a character";
	cin>>s;
	if (isDigit(s))
      cout << s<<" is a digit" << endl;
    else
      cout << s<<" is not a digit"<< endl;
}
